###  https://w3id.org/ocqa/adaptive#dependsOn
:dependsOn rdf:type owl:ObjectProperty ;
           rdfs:label "depends on"@en ,
                      "hängt ab von"@de ;
           rdfs:comment "Relates a context to another context it depends on."@en ;
           rdfs:domain :Context ;
           owl:equivalentProperty dicc:dependsOn ;
           rdfs:range :Context .


###  https://w3id.org/ocqa/adaptive#hasContent
:hasContent rdf:type owl:ObjectProperty ;
            rdfs:label "has content"@en ,
                       "hat Inhalt"@de ;
            rdfs:comment "Links a context to the resource that represents its content."@en ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain :Context ;
            owl:equivalentProperty dicc:hasContent ;
            rdfs:range rdfs:Resource .


###  https://w3id.org/ocqa/adaptive#hasContext
:hasContext rdf:type owl:ObjectProperty ;
            rdfs:label "has context"@en ,
                       "besitzt Kontext"@de ;
            rdfs:comment "Links a context collection to a contained context."@en ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain :ContextCollection ;
            owl:equivalentProperty dicc:hasContext ;
            rdfs:range :Context .


###  https://w3id.org/ocqa/adaptive#nextContext
:nextContext rdf:type owl:ObjectProperty ;
             rdfs:label "next context"@en ,
                        "nächster Kontext"@de ;
             rdfs:comment "Relates a context to the next context in a sequence."@en ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain :Context ;
             owl:equivalentProperty dicc:nextContext ;
             rdfs:range :Context .


